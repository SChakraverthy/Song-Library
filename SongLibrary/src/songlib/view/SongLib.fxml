<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.TextFlow?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="songlib.view.SongLibController">
   <bottom>
      <TextFlow prefHeight="100.0" BorderPane.alignment="CENTER" />
   </bottom>
   <center>
      <ListView fx:id="listView" prefHeight="200.0" minWidth="250.0" prefWidth="250.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <HBox alignment="CENTER_LEFT" fillHeight="false" prefHeight="50.0" prefWidth="200.0" spacing="10.0">
         <children>
            <Button fx:id="add" text="Add" />
            <Button fx:id="delete" text="Delete" />
            <Button fx:id="edit" text="Edit" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </top>
   <right>
      <TextFlow prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
